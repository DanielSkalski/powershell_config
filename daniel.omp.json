{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": false,
  "osc99": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "root",
          "style": "plain",
          "foreground": "#f1a61a",
          "properties": {
            "template": "\uF0E7"
          }
        },
        {
          "type": "os",
          "style": "plain",
          "foreground": "#fafafa",
          "properties": {
            "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}\uE0B1",
            "macos": "\uE711",
            "linux": "\uE712",
            "windows": "\uE70F"
          }
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "#fafafa",
          "properties": {
            "prefix": "<#e5c07b> \uE5FF </>",
            "style": "agnoster_short",
            "max_depth": 4,
            "enable_hyperlink": true,
            "home_icon": "\uF7DB ",
            "folder_icon": "<#e5c07b>\uE5FE </>",
            "folder_separator_icon": "<#e5c07b> \uE0B1 </>"
          }
        },
        {
          "type": "git",
          "style": "diamond",
          "leading_diamond": "\uE0D4",
          "trailing_diamond": "\uE0D2",
          "foreground": "#011627",
          "background": "#FFFB38",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ffeb95{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#c5e478{{ end }}",
            "{{ if gt .Ahead 0 }}#C792EA{{ end }}",
            "{{ if gt .Behind 0 }}#C792EA{{ end }}"
          ],
          "properties": {
            "branch_icon": "\uE725 ",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uF044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#ef5350> \uF046 {{ .Staging.String }}</>{{ end }}"
          }
        },
        {
          "type": "exit",
          "style": "plain",
          "foreground": "#fafafa",
          "background": "#ff8080",
          "properties": {
            "template": "\uE20F"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "node",
          "style": "diamond",
          "leading_diamond": " \uE0B6",
          "trailing_diamond": "\uE0B4",
          "foreground": "#ffffff",
          "background": "#3C873A",
          "properties": {
            "prefix": "\uE60C ",
            "postfix": "",
            "fetch_package_manager": true,
            "yarn_icon": " <#348cba>\uF61A</>",
            "npm_icon": " <#cc3a3a>\uE616</> "
          }
        },
        {
          "type": "python",
          "style": "diamond",
          "leading_diamond": " \uE0B6",
          "trailing_diamond": "\uE0B4",
          "foreground": "#FFE873",
          "background": "#306998",
          "properties": {
            "display_mode": "files",
            "prefix": "\uE606 ",
            "postfix": ""
          }
        },
        {
          "type": "dotnet",
          "style": "diamond",
          "leading_diamond": " \uE0B6",
          "trailing_diamond": "\uE0B4",
          "foreground": "#ffffff",
          "background": "#512cd4",
          "properties": {
            "prefix": ".net ",
            "postfix": ""
          }
        },
        {
          "type": "go",
          "style": "diamond",
          "leading_diamond": " \uE0B6",
          "trailing_diamond": "\uE0B4",
          "foreground": "#06aad5",
          "background": "#ffffff",
          "properties": {
            "prefix": "\uE627 ",
            "postfix": ""
          }
        },
        {
          "type": "ruby",
          "style": "diamond",
          "leading_diamond": " \uE0B6",
          "trailing_diamond": "\uE0B4",
          "foreground": "#9c1006",
          "background": "#ffffff",
          "properties": {
            "prefix": "\uE791 ",
            "postfix": ""
          }
        },
        {
          "type": "azfunc",
          "style": "diamond",
          "leading_diamond": " \uE0B6",
          "trailing_diamond": "\uE0B4",
          "foreground": "#5398c2",
          "background": "#ffffff",
          "properties": {
            "prefix": "\uF104<#f5bf45>\uF0E7</>\uF105 ",
            "postfix": ""
          }
        },
        {
          "type": "kubectl",
          "style": "diamond",
          "leading_diamond": " \uE0B6",
          "trailing_diamond": "\uE0B4",
          "foreground": "#ffffff",
          "background": "#316ce4",
          "properties": {
            "prefix": "\uFD31 ",
            "postfix": "",
            "template": "{{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}"
          }
        },
        {
          "type": "battery",
          "style": "diamond",
          "invert_powerline": true,
          "leading_diamond": "\uE0B2",
          "trailing_diamond": "",
          "foreground": "#262626",
          "background": "#f36943",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#b8e994{{end}}",
            "{{if eq \"Discharging\" .State.String}}#fff34e{{end}}",
            "{{if eq \"Full\" .State.String}}#33DD2D{{end}}"
          ],
          "properties": {
            "discharging_icon": " ",
            "charging_icon": "\uF1E6 ",
            "charged_icon": "\uF58E ",
            "postfix": " <#262626>\uE0B2</>"
          }
        },
        {
          "type": "time",
          "style": "diamond",
          "leading_diamond": "\uE0C5",
          "trailing_diamond": "\uE0C4",
          "background": "#40c4ff",
          "foreground": "#ffffff",
          "properties": {
            "prefix": " \uF49B ",
            "time_format": "15:04"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "exit",
          "style": "plain",
          "foreground": "#ffffff",
          "properties": {
            "prefix": "\u276F",
            "display_exit_code": false,
            "always_enabled": true,
            "error_color": "#ef5350"
          }
        }
      ]
    }
  ]
}
